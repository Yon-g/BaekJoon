   "source": [
    "def factorial(n):\n",
    "    num = 1\n",
    "    for a in range(1,n+1):\n",
    "        num *= a\n",
    "    return num\n",
    "\n",
    "T = int(input())\n",
    "for _ in range(0,T):\n",
    "    a, b = map(int, input().split())\n",
    "    print(factorial(b)//(factorial(b-a)*factorial(a)))"
   ]
